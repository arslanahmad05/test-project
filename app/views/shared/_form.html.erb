<%= form_for(User.new, method: :post, class:"register-form") do |f| %>
  <div class="form-group m-0">
    <%= f.text_field :first_name, autofocus: true, autocomplete: "email", id:"email", placeholder:"First name" %>
  </div>
  <div class="form-group m-0">
    <%= f.text_field :last_name, autocomplete: "current-password", id:"pass", placeholder:"Last name" %>
  </div>
  <div class="form-group m-0">
    <%= f.text_field :url, autocomplete: "current-password", id:"pass", placeholder:"Url" %>
  </div>
  <div class="form-group form-button submit_btn m-0">
    <%= f.submit "Submit", id:"submit", class:"form-submit" %>
  </div>
<% end %>